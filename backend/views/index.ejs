                <%}%>
            </div>  
        </section>
        <!-- wheels & tyres -->
        <section class="lubrication row mt-5">
            <div class="col-4">
                <h2>Wheels & Tyres</h2>
                <img src="/img/wheel.png" alt="wheels & tyres">
            </div>
            <div class="col-lg-8 row flex-wrap justify-content-start">
