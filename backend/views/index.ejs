<%- include('partials/header.ejs') %>

<section id="home" class="hero-section">
    <div class="hero-shape">
        <!-- <img src="/img/image2.jpg" alt="" class="shape"> -->
    </div>
    <div class="container">
        <div class="row align-items-center">
            <div class="col-lg-6">
                <div class="hero-content">
                    <h1 class="wow fadeInUp mb-3 mb-lg-5" data-wow-delay=".2s">Discover the <span>Right Parts</span> for your Car </h1>
                    <p class="wow fadeInUp mb-3 mb-lg-5" data-wow-delay=".4s">
